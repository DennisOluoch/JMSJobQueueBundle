{
    "name":         "olaldennis/job-queue-bundle",
    "description":  "Allows to run and schedule Symfony 5 console commands as background jobs.",
    "keywords":     ["job","queue", "background", "cron", "scheduled"],
    "homepage":     "https://github.com/DennisOluoch/JMSJobQueueBundle",
    "type":         "symfony-bundle",
    "license":      "Apache-2.0",
    "require": {
        "php": "^7.1|^8.0.0",
        "symfony/framework-bundle": "^5.0",
        "doctrine/common": "^3.0",
        "symfony/process": "^5.0"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.3",
        "sensio/framework-extra-bundle": "*",
        "symfony/class-loader": "*",
        "symfony/yaml": "*",
        "symfony/browser-kit": "*",
        "symfony/finder": "*",
        "symfony/css-selector": "*",
        "doctrine/doctrine-bundle": "*",
        "symfony/orm-pack": "*",
        "symfony/twig-bundle": "*",
        "symfony/form": "*",
        "symfony/validator": "*",
        "phpunit/phpunit": "^8.0",
        "symfony/intl": "*"
    },
    "suggest": {
        "sensio/framework-extra-bundle": "Required when using the webinterface.",
        "symfony/twig-bundle": "Required when using the webinterface."
    },
    "autoload": {
        "psr-0": { "JMS\\JobQueueBundle": "" }
    },
    "target-dir": "JMS/JobQueueBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "5.1.x-dev"
        }
    }
}
